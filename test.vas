section .data
message: db "Hello world!\n"

section .text
return:
    mov rax, 60
    imul rdi, rax, 2
    syscall

true:
    call return

false:
    mov rax, 0
    xchg rdi, rax
    call return

_start:
    mov rax, 1
    movsx rdi, eax
    lea rsi, [rel message]
    mov rdx, 13
    syscall
    
    mov rdi, 2
    test rdi, 6
    je true
    jmp false