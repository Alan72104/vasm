string:  db "Hello, "
string2: db "world!\n"

return:
    mov rax, 60
    mov rdi, 13
    syscall

_start:
    mov rax, 2
    sub rax, 1
    mov rdi, 2
    sub rdi, rax
    lea rsi, [rel string]
    mov rdx, 7
    syscall

    push 1
    pop rax
    mov rdi, 0
    add rdi, rax
    mov rsi, string2
    mov rdx, 7
    syscall

    call return
