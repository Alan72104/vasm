message: db "Hello world!\n"

return:
    mov rax, 60
    mov rdi, 4
    neg rdi
    syscall

true:
    call return

false:
    mov rax, 0
    xchg rdi, rax
    call return

_start:
    mov rax, 1
    movsx rdi, eax
    mov rsi, message
    mov rdx, 13
    syscall
    
    mov rdi, 2
    test rdi, 6
    je true
    jmp false