string:  db "Hello, "
string2: db "world!\n"

return:
    mov rax, 60
    mov rdi, 13
    syscall

_start:
    mov rax, 1
    mov rdi, 1
    lea rsi, [rel string]
    mov rsi, string
    mov rdx, 7
    syscall

    mov rax, 1
    mov rdi, 1
    mov rsi, string2
    mov rdx, 7
    syscall

    call return
